  <div class="pageheader">
                <h1>Setting</h1>
                <div class="breadcrumb-wrapper hidden-xs">
                    <span class="label">You are here:</span>
                    <ol class="breadcrumb">
                        <li><a href="/">Dashboard</a>
                        </li>
                        <li>Forms</li>
                        <li class="active">Series Setting</li>
                    </ol>
                </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
                <div class="row">
                    <div class="col-md-12">
                        <div class=" panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title">Series Setting</h3>
                                <div class="actions pull-right">
                                 <i class="fa fa-expand"></i>
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>
                            <div class="panel-body">
                            <%= form_for :setting,method: :patch do |f| %>
                                  
                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-10 control-label">Sale Bill Series</label>
                                            <%= f.text_field :s_series, class: "form-control" ,placeholder: "SaleSeries."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-10 control-label">Export Bill Series</label>
                                            <%= f.text_field :export_sale_series, class: "form-control" ,placeholder: "ExportSeries."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-20 control-label">Exempt/Nill Rate Series</label>
                                            <%= f.text_field :nillrated_sale_series, class: "form-control" ,placeholder: "Exempt/Nill Rate Series."%>
                                        </div>
                                    </div>
                                

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-20 control-label">C/D Note Series(Issue)</label>
                                            <%= f.text_field :cd_series, class: "form-control" ,placeholder: "C/D Note Series(Issue)"%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-20 control-label">Receipt Voucher Series</label>
                                            <%= f.text_field :rv_series, class: "form-control" ,placeholder: "Receipt Voucher Series."%>
                                        </div>
                                    </div>


                                     <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-20 control-label">JobWork (outward)</label>
                                            <%= f.text_field :jw_series, class: "form-control" ,placeholder: "JobWork (outward)."%>
                                        </div>
                                        <br><br><br>
            
                                    </div>

                                    <div class="form-group">
                                        <div class="col-sm-offset-3 col-sm-12">
                                            <button type="submit" class="btn btn-primary">Update</button>
                                        </div>
                                    </div>
                                 
                                <%end%>
                            </div>
                        </div>
                    </div>
                </div>
        </section>

       