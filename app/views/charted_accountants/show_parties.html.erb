             <div class="pageheader">
              <h1>Parties Details</h1>
              <div class="breadcrumb-wrapper hidden-xs">
                <span class="label">You are here:</span>
                <ol class="breadcrumb">
                  <li><a href="/">Dashboard</a>
                  </li>
                  <li>Tables</li>
                  <li class="active">View Details</li>
                </ol>
              </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
              <div class="row">
                <div class="col-md-12">
                  <div class=" panel panel-info">
                    <div class="panel-heading">
                      <h3 class="panel-title">Parties Details</h3>
                      <div class="actions pull-right">
                       <i class="fa fa-expand"></i>
                       <i class="fa fa-chevron-down"></i>
                       <i class="fa fa-times"></i>
                     </div>
                   </div>
                   <div class="panel-body">
                     <%= link_to 'Back', charted_accountants_path, class: 'btn btn-primary' %>
                    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                      <thead>
                        <tr>
                        <th>Sr No</th>
                         <th>Party name</th>
                         <th>Party gstin no</th>
                          <th>Party arn no</th>
                          <th>Commodity name</th>
                        </tr>
                      </thead>

                      <tbody>
                        <%@parties.each_with_index do |parties, i|%>      
                        <tr>
                          <td><%=i+1%></td>
                         <td><%=  parties.party_name %></td>
                          <td><%= parties.party_gstin_no %></td>
                          <td><%= parties.party_arn_no %></td>
                         <td><%= parties.commodity_name %></td>
                      </tr>
                      <% end %>
                    </tbody>
                  </table>

                </div>
              </div>
            </div>
          </div>
        </section>


        <% content_for :javascript do %>
        <!--Load these page level functions-->
        <script>
          $(document).ready(function() {
           $('#example').dataTable();
         });
       </script>
       <% end %>