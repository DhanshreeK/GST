<%if current_user.role == 'Party' && current_user.party.nil?%>
                            <h3><font color="red">Your Account is Deactivated Please Contact Administrator</font></h3>



                        <li class="<%= 'active' if controller?('ui') && action?('buttons') %>">
                                <a href="/parties/new">
                                    <i class="fa fa-fw fa-file"></i> Fill Profile
                                </a>
                        </li>
<%else%>


                    <%if current_user.role == 'Party' && current_user.party.party_status == true && current_user.party.party_type == 'Regular'%>

                

                    <section class="main-content-wrapper">
                            <div class="pageheader">
                                <h1>Dashboard</h1>
                                <p class="description">Welcome to GST Application</p>
                                <div class="breadcrumb-wrapper hidden-xs">
                                    <span class="label">You are here:</span>
                                    <ol class="breadcrumb">
                                        <li class="active">Dashboard</li>
                                    </ol>
                                </div>
                            </div>
                            <section id="main-content" class="animated fadeInUp">
                                <div class="row">
                                    <div class="col-md-12 col-lg-6">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="panel panel-solid-success widget-mini">
                                                    <div class="panel-body">
                                                        <i class="icon-bar-chart"></i>
                                                        <span class="total text-center"><%=current_user.invoices.count%></span>
                                                        <span class="title text-center"><%= link_to "Sale Bill GSTR1", invoices_path%></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="panel widget-mini">
                                                    <div class="panel-body">
                                                        <i class="icon-support"></i>
                                                        <span class="total text-center"><%=current_user.purchase_bills.count%></span>
                                                        <span class="title text-center"><%= link_to "Purchase Bill", purchase_bills_path%></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="panel widget-mini">
                                                    <div class="panel-body">
                                                        <i class="icon-envelope-open"></i>
                                                        <span class="total text-center"><%= current_user.credit_debit_notes.count%></span>
                                                        <span class="title text-center"><%= link_to "Credit Debit Note", credit_debit_notes_path%></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="panel panel-solid-info widget-mini">
                                                    <div class="panel-body">
                                                        <i class="icon-user"></i>
                                                        <span class="total text-center"><%=current_user.export_invoices.count%></span>
                                                        <span class="title text-center"><%= link_to "Export Invoice GSTR1", '/export_invoices'%></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                               <%elsif current_user.role == 'Party' && current_user.party.party_status == true && current_user.party.party_type == 'Composite'%> 
                                <%elsif current_user.role == 'SuperAdmin' %>
                                <%elsif current_user.role == 'ChartedAccountant' %>
                                <%else%>
                                <h3><font color="red">Your Account is Deactivated Please Contact Administrator</font></h3>
                                <%end%>


    <%end%>