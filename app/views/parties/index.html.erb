             <div class="pageheader">
              <h1>Party Details</h1>
              <div class="breadcrumb-wrapper hidden-xs">
                <span class="label">You are here:</span>
                <ol class="breadcrumb">
                  <li><a href="/">Dashboard</a>
                  </li>
                  <li>Tables</li>
                  <li class="active">View Details</li>
                </ol>
              </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
              <div class="row">
                <div class="col-md-12">
                  <div class=" panel panel-info">
                    <div class="panel-heading">
                      <h3 class="panel-title">Party Details</h3>
                      <div class="actions pull-right">
                       <i class="fa fa-expand"></i>
                       <i class="fa fa-chevron-down"></i>
                       <i class="fa fa-times"></i>
                     </div>
                   </div>
                   <div class="panel-body">
                    <%if current_user.role == 'Party'%>
                          <%else%>
                             <%= link_to 'New Party', new_party_path, class: 'btn btn-primary' %>
                          <%end%>
                    <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                      <thead>
                        <tr>
                          <th>SR NO</th>
                           <th>Party name</th>
                            <th>Party gstin no</th>
                             <th>Party arn no</th>
                              <th>State</th>
                        </tr>
                      </thead>

                      <tbody>
                         <%if current_user.role == 'Party'%>
                                
                                  <tr>
                                 
                                      <td><%= link_to @parties.party_name, party_path(@parties), class: 'btn btn-link' %></td>
                                      <td><%= @parties.party_gstin_no %></td>
                                      <td><%= @parties.party_arn_no %></td>
                                      <td><%= @parties.commodity_name %></td>
                                       <td><%= @parties.state_name %></td>
                                       </tr>
                                <%else%>  

                                  <% @parties.each do |party, i| %>
                                    <tr>
                                     
                                      <td><%= link_to party.party_name, party_path(party), class: 'btn btn-link' %></td>
                                      <td><%= party.party_gstin_no %></td>
                                      <td><%= party.party_arn_no %></td>
                                      <td><%= party.commodity_name %></td>
                                       <td><%= party.state_name %></td>
                                     
                                    </tr>
                                  <% end %>
                                  <% end %>
                    </tbody>
                  </table>

                </div>
              </div>
            </div>
          </div>
        </section>


        <% content_for :javascript do %>
        <!--Load these page level functions-->
        <script>
          $(document).ready(function() {
           $('#example').dataTable();
         });
       </script>
       <% end %>