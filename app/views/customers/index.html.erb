
                <div class="row">
                    <div class="col-md-12">

                        <div class="panel panel-default">
                          <div class="panel-heading">
                            <h3 class="panel-title">View All Customers</h3>
                            <div class="actions pull-right">
                             <i class="fa fa-expand"></i>
                                <i class="fa fa-chevron-down"></i>
                                <i class="fa fa-times"></i>
                            </div>
                          </div>
                          <div class="panel-body">
                          
                             <%= link_to 'New Customer', new_customer_path, class: 'btn btn-primary' %> |  <%= link_to 'Back', new_customer_path,class: "btn btn-primary" %>
                            <div class="table-responsive">
                              <table class="table table-bordered table-striped">
                                <thead>
                                  <tr>
                                     <th>Sr.No</th>
                                    <th>Customer name</th>
                                    <th>Gstin no reg</th>
                                    <th>Arn no</th>
                                   
                                    <th>Pan No</th>
                                    <th>Actions</th>
                                    <th colspan="3"></th>
                                  </tr>
                                </thead>

                                <tbody>
                                  <% @customers.each_with_index do |customer,i| %>
                                    <tr>
                                      <td><%= i + 1 %></td>
                                      <td><%= customer.customer_name %></td>
                                      <td><%= customer.gstin_no_reg %></td>
                                      <td><%= customer.arn_no %></td>
                                      <td><%= customer.cust_pan_no %></td>
                                      <td><%= link_to 'Show', customer %></td>
                                      <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
                                      <td><%= link_to 'Destroy', customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                                    </tr>
                                  <% end %>
                                </tbody>
                              </table>
                              </div>
                              </div>
                              </div>
                              </div>
                              </div>

                              <br>